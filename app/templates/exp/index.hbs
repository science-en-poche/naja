<div class="offset2 span4">
  <h2>Results</h2>
  <hr>
  <table class="table table-striped table-hover table-condensed">
    <thead>
      <tr>
        <th>Id</th>
        <th>By profile</th>
        <th>Created</th>
      </tr>
    </thead>
    <tbody>
      {{#each arrangedContent}}
        <tr>
          <td>{{result this}}</td>
          <td>{{profile this.profile}}</td>
          <td>{{date createdAt}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<div class="span4">
  <h2>Graph</h2>
  <hr>
</div>
